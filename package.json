{
  "name": "collaborative-software-development",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "serve": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc",
    "build:mc": "vite build",
    "build:html": "vite -c vite.publish.config.ts build",
    "deploy": "bun run deploy:normal && bun run deploy:minimal",
    "deploy:normal": "bun run build:mc && bun run build:html",
    "deploy:minimal": "bun run build:mc && bun run build:html"
  },
  "dependencies": {
    "@hhenrichsen/canvas-commons": "^0.10.2",
    "@lezer/javascript": "^1.4.21",
    "@lezer/markdown": "^1.4.2",
    "@motion-canvas/2d": "^3.17.2",
    "@motion-canvas/core": "^3.17.2",
    "@motion-canvas/ffmpeg": "^1.1.0",
    "mermaid": "^11.12.3"
  },
  "devDependencies": {
    "@motion-canvas/ui": "^3.17.2",
    "@motion-canvas/vite-plugin": "^3.17.2",
    "typescript": "^5.2.2",
    "vite": "^4.0.0"
  }
}
